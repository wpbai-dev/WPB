{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "aws/aws-sdk-php": "^3.295",
        "bacon/bacon-qr-code": "^2.0",
        "coingate/coingate-php": "^4.1",
        "cviebrock/eloquent-sluggable": "^10.0",
        "diglactic/laravel-breadcrumbs": "^8.1",
        "ezyang/htmlpurifier": "^4.17",
        "guzzlehttp/guzzle": "^7.2",
        "hashids/hashids": "^5.0",
        "intervention/image": "^2.7",
        "iyzico/iyzipay-php": "^2.0",
        "jenssegers/date": "^4.0",
        "kingflamez/laravelrave": "^4.2",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.2",
        "laravel/socialite": "^5.5",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.2.2",
        "league/flysystem-aws-s3-v3": "^3.0",
        "livewire/livewire": "2.12.8",
        "maatwebsite/excel": "^3.1",
        "mercadopago/dx-php": "^3.0",
        "mews/purifier": "^3.4",
        "midtrans/midtrans-php": "^2.5",
        "mollie/laravel-mollie": "^3.0",
        "paddlehq/paddle-php-sdk": "^1.1",
        "php-http/guzzle7-adapter": "^1.0",
        "pragmarx/google2fa-laravel": "^2.0",
        "propaganistas/laravel-disposable-email": "^2.4",
        "razorpay/razorpay": "^2.9",
        "rosell-dk/webp-convert": "^2.9",
        "socialiteproviders/envato": "^4.1",
        "socialiteproviders/microsoft": "^4.5",
        "socialiteproviders/vkontakte": "^5.0",
        "spatie/laravel-sitemap": "^7.0",
        "spatie/schema-org": "^3.23",
        "stripe/stripe-php": "^13.15",
        "trustip/trustip": "^1.1",
        "uddoktapay/laravel-sdk": "^1.0",
        "vironeer/toastr": "^1.0",
        "xendit/xendit-php": "^4.3",
        "yoomoney/yookassa-sdk-php": "^3.5"
    },
    "require-dev": {
        "spatie/laravel-ignition": "^2.0",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Vironeer\\": "vironeer/",
            "Vironeer\\Installer\\": "vironeer/installer/",
            "Vironeer\\Installer\\App\\": "vironeer/installer/app",
            "Vironeer\\ChunkUpload\\": "vironeer/chunk-upload",
            "Vironeer\\PayPal\\": "vironeer/paypal-sdk",
            "Vironeer\\NOWPayments\\": "vironeer/nowpayments"
        },
        "files": [
            "app/Helpers/General.php",
            "app/Helpers/Theme.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
